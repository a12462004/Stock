<!DOCTYPE html>
<html lang="en">
<%- include header.ejs %> 
  <body>
    <%- include content_head.ejs %> 
    <div class="container-fluid">
      <div class="row">
        <%- include sidebar.ejs %> 

        <div class="text-center">
          <div class=col-sm-10>
            <div id="upper_row" class="row">

            <div class="col-xs-6 " >
              <img src="../images/poya.png" class=" img-circle" style="float:right;width:300px; height:300;" >
            </div>  
            <div class="col-xs-6 " style="height:200px;width:400px;text-align:left;">
              <h3 class="valign">
              <% for(var i=0; i<data.length; i++) {%>
              <%= data[i].code %>
              <br/><br/><%= data[i].company %>
              <% } %>
              </h3>
            </div>
          </div>
          </div>

          <div>
            <div id="sencondTitle" class="col-sm-10">
              <h2 class="page-header" >公司基本資料</h2>
            </div>
            <div class="table-responsive col-sm-10">
              <table class="table table-striped table-bordered" style="table-layout:fixed;">
                <thead>
                <% for(var i=0; i<data.length; i++) {%>
                  <tr>
                    <th class="warning">證期會代碼</th>
                    <td><%= data[i].code %></td>
                    <th class="warning">公司名稱</th>
                    <td><%= data[i].company %></td>
                  </tr>
                  <tr>
                    <th class="warning">上市別</th>
                    <td><%= data[i].listed_type %></td>
                    <th class="warning">員工人數</th>
                    <td><%= data[i].employees %></td>
                  </tr>
                  <tr>
                    <th class="warning">產業類別編號</th>
                    <td><%= data[i].industry %></td>
                    <th class="warning">會計月份</th>
                    <td><%= data[i].accounting %></td>
                  </tr>
                  <tr>
                    <th class="warning">統一編號 </th>
                    <td><%= data[i].tax %></td>
                    <th class="warning">國際證券編號</th>
                    <td><%= data[i].isin %></td>
                  </tr>
                  <tr>
                    <th class="warning">電話</th>
                    <td><%= data[i].telephone %></td>
                    <th class="warning">傳真</th>
                    <td><%= data[i].fax %></td>
                  </tr>
                  <tr>
                    <th class="warning">網址</th>
                    <td><%= data[i].website %></td>
                    <th class="warning">電子信箱</th>
                    <td><%= data[i].e_mail %></td>
                  </tr>
                  <tr>
                    <th class="warning">董事長</th>
                    <td><%= data[i].chairman %></td>
                    <th class="warning">總經理</th>
                    <td><%= data[i].general %></td>
                  </tr>
                  <tr>
                    <th class="warning">發言人</th>
                    <td><%= data[i].spokes %></td>
                    <th class="warning">財務經理</th>
                    <td><%= data[i].controller %></td>
                  </tr>
                  <tr>
                    <th class="warning">員工人數</th>
                    <td><%= data[i].employees %></td>
                    <th class="warning">實收資本額(元)</th>
                    <td><%= data[i].capital %></td>
                  </tr>
                  <tr>
                    <th class="warning">設立日期</th>
                    <td colspan="4"><%= data[i].founded %></td> 
                  </tr>
                  <tr>
                    <th class="warning">上市日期</th>
                    <td colspan="4"><%= data[i].listed_date %></td>
                  </tr>
                    

                <% } %>
                </thead>
              </table>
            </div>
          </div>
        </div>

        </div> 
        </div>
      </div>
    </div>

    <!-- Bootstrap core JavaScript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="../../assets/js/vendor/jquery.min.js"><\/script>')</script>
   
  </body>
</html>
